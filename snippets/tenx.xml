<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote name="tenx"
          fetch="https://github.com/TenX-OS"
          revision="eleven" />

  <remote name="dark"
          fetch="https://github.com/Night-Raids-Reborn"
          revision="eleven" />

  <remote name="tenx-devices"
          fetch="https://github.com/Ten-X-Devices"
          revision="eleven" />

  <remote name="gitlab"
          fetch="https://gitlab.com" />

  <remote name="tenx-gitlab"
          fetch="https://gitlab.com/TenX-OS"
          revision="eleven" />

  <!-- TenX Repos -->

  <!-- Framework repos -->
  <project path="frameworks/base" name="tenx-frameworks_base" remote="dark" />
  <project path="frameworks/av" name="frameworks_av" remote="tenx" />

  <!-- General repos -->
  <project path="build/make" name="tenx-build_make" remote="dark">
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="tenx_build_soong" remote="dark" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>
  <project path="manifest" name="manifest_TenX" remote="tenx" />

  <!-- External repos -->
  <project path="external/faceunlock" name="android_external_faceunlock" remote="tenx-gitlab" />
  <project path="external/pulse" name="android_external_pulse" remote="tenx" />
  <project path="external/airbnb-lottie" name="external_airbnb-lottie" remote="tenx" />

  <!-- Packages repos -->
  <project path="packages/apps/OmniStyle" name="packages_apps_OmniStyle" remote="tenx" />
  <project path="packages/apps/Settings" name="packages_tenx_Settings" remote="dark" />
  <project path="packages/apps/TenX" name="packages_apps_TenX" remote="dark" />
  <project path="packages/apps/CustomDoze" name="packages_apps_CustomDoze" remote="tenx" />
  <project path="packages/apps/Updates" name="packages_apps_Updates" remote="tenx" />
  <project path="packages/services/OmniJaws" name="packages_services_OmniJaws" remote="tenx" />
  <project path="packages/services/Telephony" name="packages_services_Telephony" remote="tenx" />
  <project path="packages/services/Telecomm" name="packages_services_Telecomm" remote="tenx" />
  <project path="packages/apps/SoftAPManager" name="android_packages_apps_SoftAPManager" remote="dark" clone-depth="1" />

  <!-- System repos -->
  <project path="system/core" name="system_core" remote="tenx" />
  <project path="system/netd" name="AICP/system_netd" remote="github" revision="r11.1" />

  <!-- Sepolicy repos -->
  <project path="device/custom/sepolicy" name="device_tenx_sepolicy" remote="dark" />
  <project path="system/sepolicy" name="system_sepolicy" remote="tenx" />

  <!-- Vendor repos -->
  <project path="vendor/aosp" name="vendor_TenX" remote="dark" />
  <project path="vendor/support" name="vendor_TenX_support" remote="tenx" />
  <project path="vendor/overlays/Elegance" name="vendor_overlays_Elegance" remote="dark" />
  <!-- General repos -->
  <project path="vendor/gapps" name="vendor_gapps" remote="tenx-gitlab" clone-depth="1" />
  <project path="vendor/lawnchair" name="Night-Raids-Reborn/android_vendor_lawnchair" remote="github" revision="twelve" clone-depth="1" />

  <!-- Other additions -->
  <project path="external/hosts" name="StevenBlack/hosts" remote="github" revision="master" />
  <project path="packages/modules/DnsResolver" name="AICP/packages_modules_DnsResolver" groups="pdk-cw-fs,pdk-fs" remote="github" revision="r11.1" />

</manifest>
